@model IEnumerable<Whoops.DataLayer.Trip>


@{
    ViewBag.Title = "Home Page";

}
<div class="row">
    <div class="col-md-6">
        <h1>Super content</h1>
        <div>
            <form method="post">
                <div class="form-group">
                    <label>Name</label>
                    <input class="form-control" />
                    <span style="color:red" class="text-muted" ></span>
                </div>
                <div class="form-group">
                    <label >Location</label>
                    <input  class="form-control" />
                    <span style="color:red" class="text-muted" ></span>
                </div>
                <div class="form-group">
                    <label >Date</label>
                    <input  class="form-control" />
                    <span style="color:red" class="text-muted" ></span>
                </div>
                <div class="form-group">
                    <a asp-controller="App" asp-action="Index" class="btn btn-primary">Cancel</a>
                    <input type="submit" value="Send" class="btn btn-success" />
                </div>
            </form>
        </div>
    </div>
    <div class="col-md-6">
        <h1>The map</h1>
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Name</li>
            }
        </ul>
    </div>
</div>
